<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.atomsyndication"
    purge="False">
    <configlet title="Atom Syndication"
        action_id="atom_syndication"
        appId="collective.atomsyndication"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@atom_settings"
        icon_expr="string:${portal_url}/++resource++collective.atomsyndication/16px-Feed-icon.svg.png"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>
</object>
